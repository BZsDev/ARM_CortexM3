{
    "update.mode": "none",
    "telemetry.telemetryLevel": "off",
    "workbench.startupEditor": "none",
    "update.showReleaseNotes": false,
    "update.enableWindowsBackgroundUpdates": false,
    "typescript.surveys.enabled": false,
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "workbench.colorTheme": "Default Light Modern",
    "cSpell.userWords": [
        "ANGLECONCEPT",
        "ASIL",
        "ISMPC"
    ],

    "C_Cpp.inactiveRegionOpacity": 0.72,

    "workbench.colorCustomizations": {
        "editor.background": "#ffffff3f",
        "editor.lineHighlightBorder": "#c0c0c0",
        "editorBracketMatch.border": "#cc0000",
        "editor.lineHighlightBackground": "#1e95aa27",
        "editorWhitespace.foreground": "#c0c0c0"
    },
 
 
 
 
    "editor.tokenColorCustomizations": {
        "types":"#BF0000",
        "variables": "#000000",
        "functions": "#0000ff",
 
        "textMateRules": [
            {
                "scope": [
                    "keyword.other.unit.suffix",
                    "keyword.other.unit.hexadecimal",
                    "keyword.other.unit.binary",
                    "keyword.other.unit.octal",
                    "constant.numeric",
                    "string.quoted.double",
                    "string.quoted.single",
                    "constant.character.escape",
                    "constant.character.format.placeholder.other",
                    "variable.other.enummember",
                ],
                "settings": {
                    "foreground": "#0089a1"
                }
            },
            {
                "scope": [
                    "comment.block.documentation",
                    "comment.block",
                    "comment.line.number-sign"
                ],
                "settings": {
                    "foreground": "#008f00"
                }
            },
            {
                "scope": [
                    "entity.name.function.preprocessor",
                    "entity.other.attribute-name.pragma.preprocessor",
                ],
                "settings": {
                    "foreground": "#2abda9"
                }
            },
            {
                "scope": [
                    "string.quoted.other.lt-gt.include",
                    "string.quoted.double.include"
                ],
                "settings": {
                    "foreground": "#5e32d8"
                }
            },
            {
                "scope": [
                    //"entity.name.function.preprocessor",
                    //"entity.other.attribute-name.pragma.preprocessor",
                    "keyword.control.directive.conditional",
                    "keyword.control.directive.define",
                    "keyword.control.directive.endif",
                    "keyword.control.directive.undef",
                    "keyword.control.directive.include",
                    "keyword.control.directive.diagnostic.error",
                    "keyword.control.directive.diagnostic.warning",
                    "keyword.control.directive.pragma",
                    //"string.quoted.other.lt-gt.include",
                    //"string.quoted.double.include"
                ],
                "settings": {
                    "foreground": "#d10707"
                }
            },
            {
                "scope": [
                    "keyword.control.switch",
                    "storage.modifier",
                    "storage.type.built-in.primitive",
                    "storage.type.function",
                    "storage.type.struct",
                    "storage.type.enum",
                    "storage.type.union",
                    "storage.type.asm",
                    "storage.type.extern",
                    "keyword.control.case",
                    "keyword.control.default",
                    "keyword.control",
                    "keyword.other.typedef",
                    "punctuation.terminator",
                    "punctuation.separator",
                    "support.function.builtin",
                    "constant.language"
                ],
                "settings": {
                    "foreground": "#7f00ff"
                }
            },
            {
                "scope": [
                    "keyword.operator",
                    "meta.function-call.generic"
                ],
                "settings": {
                    "foreground": "#0000ff",
                }
            },
            {
                "scope": [
                    "variable.other.local.c",
                    "variable.parameter.c",
                ],
                "settings": {
                    "foreground": "#000000",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "variable.other.property.c",
                    "variable.parameter.c",
                ],
                "settings": {
                    "foreground": "#000000",
                }
            },
            {
                "scope": [
                    "constant.other.caps",
                ],
                "settings": {
                    "foreground": "#000000",
                }
            }
        ]
    },
    "cmake.configureOnOpen": false,
}
